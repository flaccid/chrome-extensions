{
  "manifest_version": 2,
  
  "name": "RightScale Omnibox",
  "version": "1.0",
  "description": "Navigates you to RightScale's dashboard.",
  
  "icons": {
    "16": "icon_16.png",
    "48": "icon_48.png",
    "128": "icon_128.png"
  },
  
  "homepage_url": "http://rightscale.com/",

  "background": {
    "scripts": ["background.js"]
  },

  "browser_action": {
    "default_icon": "icon.png"
  },
  
  "permissions": ["tabs"]

}